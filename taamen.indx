    <style>
        /* --- 1. Variables & Base --- */
        :root {
            --bg-deep: #02020a;
            --accent-cyan: #00f2ff;
            --accent-green: #00ff41; 
            --accent-gold: #ffd700;
            --danger: #ff0040;
            --purple-neon: #bc13fe;
            --glass-bg: rgba(255, 255, 255, 0.05);
            --glass-border: rgba(255, 255, 255, 0.1);
            --scanline-color: rgba(18, 16, 16, 0.1);
            --transition-speed: 0.5s;
        }

        * { margin: 0; padding: 0; box-sizing: border-box; }
        
        body {
            font-family: 'Tajawal', sans-serif;
            background: var(--bg-deep);
            color: white;
            overflow-x: hidden;
            min-height: 100vh;
            perspective: 1200px;
            transform-style: preserve-3d;
            scrollbar-width: thin;
            scrollbar-color: var(--accent-cyan) var(--bg-deep);
        }

        /* Custom Scrollbar for Webkit */
        ::-webkit-scrollbar { width: 6px; }
        ::-webkit-scrollbar-track { background: var(--bg-deep); }
        ::-webkit-scrollbar-thumb { background: var(--accent-cyan); border-radius: 10px; }

        /* --- 2. Animations Keyframes --- */
        @keyframes glitch-anim { 
            0% { clip: rect(31px, 9999px, 94px, 0); } 
            20% { clip: rect(6px, 9999px, 86px, 0); } 
            40% { clip: rect(68px, 9999px, 2px, 0); } 
            60% { clip: rect(13px, 9999px, 63px, 0); } 
            80% { clip: rect(98px, 9999px, 20px, 0); } 
            100% { clip: rect(38px, 9999px, 16px, 0); } 
        }
        
        @keyframes glitch-skew { 
            0% { transform: skew(3deg); } 10% { transform: skew(-2deg); } 
            20% { transform: skew(1deg); } 30% { transform: skew(-1deg); } 
            40% { transform: skew(3deg); } 50% { transform: skew(-3deg); } 
            100% { transform: skew(0deg); } 
        }

        @keyframes pulse-gold {
            0% { box-shadow: 0 0 10px var(--accent-gold); border-color: var(--accent-gold); }
            100% { box-shadow: 0 0 30px var(--accent-gold), 0 0 50px rgba(255, 215, 0, 0.3); border-color: white; }
        }

        @keyframes floating {
            0%, 100% { transform: translateY(0); }
            50% { transform: translateY(-15px); }
        }

        /* --- 3. UI Components --- */
        
        /* Boot Screen */
        #bootScreen {
            position: fixed; top: 0; left: 0; width: 100%; height: 100%;
            background: black; z-index: 10000; display: flex; flex-direction: column;
            justify-content: center; align-items: center; font-family: 'Orbitron', monospace; color: var(--accent-green);
        }
        .boot-text-container { width: 350px; font-size: 0.9rem; line-height: 1.6; position: relative; }
        
        /* Home Button */
        .home-btn {
            position: fixed; top: 25px; right: 25px; width: 60px; height: 60px;
            background: var(--glass-bg); border-radius: 15px; display: none;
            justify-content: center; align-items: center; z-index: 9000; cursor: pointer;
            backdrop-filter: blur(15px); border: 1px solid var(--glass-border);
            transition: 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275); font-size: 1.8rem;
            color: white; text-shadow: 0 0 10px var(--accent-cyan);
        }
        .home-btn:hover { 
            background: var(--accent-cyan); color: black; 
            transform: scale(1.1) rotate(10deg); 
            box-shadow: 0 0 30px var(--accent-cyan);
        }

        /* Scanlines Effect */
        .scanlines {
            position: fixed; top: 0; left: 0; width: 100%; height: 100%;
            background: linear-gradient(var(--scanline-color) 50%, rgba(0, 0, 0, 0) 50%);
            background-size: 100% 4px; z-index: 8000; pointer-events: none; opacity: 0.2;
        }

        /* Canvases for Backgrounds */
        canvas { position: fixed; top: 0; left: 0; width: 100%; height: 100%; z-index: -1; pointer-events: none; }
        #matrixCanvas { display: none; opacity: 0.3; }
        #particlesCanvas { display: block; opacity: 0.6; }
        #starCanvas { display: none; }

        /* --- 4. Main Layout & Pages --- */
        .page { 
            display: none; padding: 40px 20px 150px; 
            max-width: 1200px; margin: 0 auto; 
            animation: pageReveal 0.8s cubic-bezier(0.23, 1, 0.32, 1) forwards; 
            position: relative; z-index: 10; 
        }
        .page.active { display: block; }

        @keyframes pageReveal {
            from { opacity: 0; transform: translateZ(-200px) translateY(50px); filter: blur(10px); }
            to { opacity: 1; transform: translateZ(0) translateY(0); filter: blur(0); }
        }

        /* Brand Header */
        .brand-header { text-align: center; margin-bottom: 50px; padding-top: 60px; }
        .year-tag { font-family: 'Orbitron'; letter-spacing: 12px; color: var(--accent-gold); font-size: 1.2rem; display: block; margin-bottom: 10px; opacity: 0.8; }
        
        .glitch-text {
            font-size: 6rem; font-weight: 900; position: relative; color: white;
            text-shadow: 0 0 20px var(--accent-cyan), 0 0 40px var(--accent-cyan);
            animation: glitch-skew 1s infinite linear alternate-reverse;
            font-family: 'Tajawal';
        }
        .glitch-text::before, .glitch-text::after {
            content: attr(data-text); position: absolute; left: 0; width: 100%; height: 100%; opacity: 0.8;
        }
        .glitch-text::before { color: var(--danger); z-index: -1; animation: glitch-anim 3s infinite linear alternate-reverse; }
        .glitch-text::after { color: var(--accent-cyan); z-index: -2; animation: glitch-anim 2s infinite linear alternate-reverse; }

        /* Navigation Grid */
        .nav-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); gap: 25px; margin-top: 40px; }
        
        .menu-card {
            background: linear-gradient(145deg, rgba(255,255,255,0.05), rgba(0,0,0,0.3));
            border: 1px solid var(--glass-border); border-radius: 25px; padding: 40px;
            text-align: center; transition: 0.5s cubic-bezier(0.4, 0, 0.2, 1); 
            cursor: pointer; position: relative; overflow: hidden;
            transform-style: preserve-3d;
        }
        .menu-card::before {
            content: ''; position: absolute; top: 0; left: -100%; width: 100%; height: 100%;
            background: linear-gradient(90deg, transparent, rgba(255,255,255,0.1), transparent);
            transition: 0.5s;
        }
        .menu-card:hover::before { left: 100%; }
        
        .menu-card:hover {
            border-color: var(--accent-cyan);
            transform: translateY(-15px) rotateX(10deg) rotateY(5deg);
            box-shadow: 0 20px 50px rgba(0, 242, 255, 0.2);
            background: rgba(255,255,255,0.08);
        }

        .menu-card.premium-card {
            grid-column: 1 / -1;
            border: 2px solid var(--accent-gold);
            background: linear-gradient(135deg, rgba(255, 215, 0, 0.05), rgba(255, 0, 64, 0.05));
            animation: pulse-gold 3s infinite alternate;
        }

        .card-icon { font-size: 3.5rem; margin-bottom: 20px; display: block; filter: drop-shadow(0 0 10px currentColor); }
        .menu-card h2 { font-size: 1.8rem; margin-bottom: 10px; font-weight: 800; }
        .menu-card p { color: #aaa; font-size: 1rem; line-height: 1.4; }

        /* --- 5. Specific Page Styles --- */

        /* Warriors Page (VS Arena) */
        .vs-arena { display: flex; flex-direction: column; align-items: center; gap: 40px; margin-top: 50px; }
        .battle-grid { display: grid; grid-template-columns: 1fr auto 1fr; gap: 40px; width: 100%; align-items: center; }
        
        .warrior-list { display: flex; flex-direction: column; gap: 15px; width: 100%; }
        .warrior-card {
            background: var(--glass-bg); border: 1px solid var(--glass-border);
            padding: 20px 30px; border-radius: 50px; font-weight: 800; font-size: 1.3rem;
            text-align: center; transition: 0.4s; position: relative; overflow: hidden;
            display: flex; justify-content: space-between; align-items: center;
        }
        .team-left .warrior-card { border-right: 5px solid var(--accent-cyan); text-align: right; flex-direction: row-reverse; }
        .team-right .warrior-card { border-left: 5px solid var(--danger); text-align: left; }

        .warrior-card:hover {
            background: white; color: black; transform: scale(1.05) translateX(10px);
            box-shadow: 0 0 30px white;
        }
        .team-left .warrior-card:hover { transform: scale(1.05) translateX(-10px); }

        .vs-divider {
            font-size: 5rem; font-weight: 900; color: var(--accent-gold);
            font-family: 'Orbitron'; text-shadow: 0 0 30px var(--accent-gold);
            animation: floating 3s infinite ease-in-out;
        }

        /* Stats Page */
        .stats-wrapper { display: grid; grid-template-columns: repeat(auto-fit, minmax(400px, 1fr)); gap: 30px; }
        .stats-box {
            background: rgba(0,0,0,0.5); border: 1px solid var(--glass-border);
            border-radius: 30px; padding: 30px; backdrop-filter: blur(10px);
        }
        .stats-title { font-size: 1.8rem; font-weight: 900; margin-bottom: 25px; text-align: center; color: var(--accent-cyan); }
        
        .data-table { width: 100%; border-collapse: separate; border-spacing: 0 10px; }
        .data-table tr { transition: 0.3s; }
        .data-table tr:hover { transform: scale(1.02); background: rgba(255,255,255,0.05); }
        .data-table td { padding: 15px; background: rgba(255,255,255,0.02); }
        .data-table td:first-child { border-radius: 15px 0 0 15px; font-weight: 800; }
        .data-table td:last-child { border-radius: 0 15px 15px 0; text-align: center; color: var(--accent-cyan); font-family: 'Orbitron'; font-weight: 700; }

        /* Injuries Page */
        .injury-card {
            background: linear-gradient(45deg, rgba(255,0,64,0.1), transparent);
            border: 1px solid var(--danger); border-radius: 20px; padding: 25px;
            margin-bottom: 20px; display: flex; justify-content: space-between; align-items: center;
            transition: 0.4s;
        }
        .injury-card:hover { transform: scale(1.02); box-shadow: 0 0 20px var(--danger); }
        .player-info h3 { font-size: 1.5rem; margin-bottom: 5px; }
        .player-info p { color: #ccc; }
        .status-badge {
            padding: 8px 20px; border-radius: 50px; font-size: 0.8rem; font-weight: 900;
            text-transform: uppercase; letter-spacing: 1px;
        }
        .status-danger { background: var(--danger); color: white; animation: blink 1s infinite; }
        .status-warning { background: var(--accent-gold); color: black; }

        /* Live Page */
        .live-monitor { text-align: center; }
        .big-timer { 
            font-size: 7rem; font-weight: 900; font-family: 'Orbitron'; 
            color: white; text-shadow: 0 0 50px var(--accent-cyan);
            margin: 40px 0;
        }
        .live-ticker {
            background: #000; color: var(--accent-green); padding: 15px;
            border: 2px solid var(--accent-green); border-radius: 10px;
            font-family: monospace; overflow: hidden; position: relative;
        }
        .ticker-move { display: inline-block; white-space: nowrap; animation: ticker 20s linear infinite; }
        @keyframes ticker { 
            from { transform: translateX(100%); }
            to { transform: translateX(-100%); }
        }

        /* Decisions Page */
        .decision-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(320px, 1fr)); gap: 20px; }
        .decision-card {
            background: rgba(188, 19, 254, 0.05); border: 1px solid var(--purple-neon);
            border-radius: 20px; padding: 30px; position: relative; overflow: hidden;
            transition: 0.4s;
        }
        .decision-card:hover { background: rgba(188, 19, 254, 0.15); transform: translateY(-10px); box-shadow: 0 0 30px var(--purple-neon); }
        .decision-badge {
            position: absolute; top: 15px; left: 15px; background: var(--purple-neon);
            color: white; padding: 5px 15px; border-radius: 50px; font-size: 0.7rem; font-weight: 900;
        }
        .decision-card h3 { font-size: 2rem; margin: 20px 0 10px; color: var(--purple-neon); font-weight: 900; }
        .decision-text { line-height: 1.6; color: #eee; font-size: 1.1rem; }

        /* Elite Room */
        .cyber-box {
            background: rgba(0, 255, 65, 0.02); border: 1px solid var(--accent-green);
            padding: 40px; border-radius: 5px; font-family: monospace;
            box-shadow: inset 0 0 30px rgba(0, 255, 65, 0.1);
        }
        .command-line { color: var(--accent-green); margin-bottom: 10px; display: block; }
        .command-line::before { content: "> "; }
        .cyber-btn {
            display: block; width: 100%; padding: 20px; margin-top: 20px;
            border: 1px solid var(--accent-green); color: var(--accent-green);
            text-align: center; text-decoration: none; font-weight: bold;
            transition: 0.3s; background: transparent;
        }
        .cyber-btn:hover { background: var(--accent-green); color: black; box-shadow: 0 0 20px var(--accent-green); }

        /* --- WEATHER PAGE STYLES --- */
        .weather-display-container {
            display: flex; flex-direction: column; align-items: center; text-align: center;
        }
        .weather-main-circle {
            width: 280px; height: 280px; border-radius: 50%;
            background: radial-gradient(circle, rgba(0,242,255,0.1) 0%, rgba(0,0,0,0) 70%);
            border: 2px solid var(--accent-cyan);
            display: flex; flex-direction: column; justify-content: center; align-items: center;
            box-shadow: 0 0 50px rgba(0,242,255,0.2);
            animation: pulse-gold 4s infinite alternate;
            margin-bottom: 40px;
            backdrop-filter: blur(5px);
        }
        .weather-temp { font-size: 5rem; font-weight: 900; color: white; font-family: 'Orbitron'; line-height: 1; }
        .weather-desc { font-size: 1.5rem; color: var(--accent-cyan); margin-top: 10px; }
        
        .weather-grid {
            display: grid; grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            gap: 20px; width: 100%; max-width: 900px;
        }
        .weather-stat-card {
            background: rgba(255,255,255,0.05); padding: 20px; border-radius: 15px;
            border: 1px solid rgba(255,255,255,0.1); text-align: center;
        }
        .w-icon { font-size: 2rem; margin-bottom: 10px; display: block; }
        .w-val { font-size: 1.5rem; font-weight: bold; color: var(--accent-gold); font-family: 'Orbitron'; }
        .w-label { color: #aaa; font-size: 0.9rem; }

        .field-status {
            margin-top: 40px; padding: 20px; width: 100%; max-width: 900px;
            border: 1px dashed var(--danger); background: rgba(255, 0, 64, 0.05);
            color: var(--danger); font-weight: bold; font-size: 1.2rem;
            text-align: center; border-radius: 10px;
        }

        /* --- 6. Mobile Responsiveness --- */
        @media (max-width: 768px) {
            .glitch-text { font-size: 3.5rem; }
            .battle-grid { grid-template-columns: 1fr; }
            .vs-divider { transform: rotate(90deg); margin: 20px 0; font-size: 3.5rem; }
            .big-timer { font-size: 3.5rem; }
            .nav-grid { grid-template-columns: 1fr; }
            .warrior-card { font-size: 1.1rem; }
            .weather-main-circle { width: 200px; height: 200px; }
            .weather-temp { font-size: 3.5rem; }
        }

    </style>
</head>
